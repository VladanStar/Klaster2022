class prasad
{
    public static void main (String[] args)
    {
        // Area of circle
        int r,area;
        r=5;
        area= 3.14 *r*r;
        System.out.println("Area of Circle: " +area);

        // Area of squre
        
         int a,SqArea;
        a=5;
        sqArea= a*a;
        System.out.println("Area of Squre: " +sqArea);




    }
}